    "cross-env": "^7.0.3",
    "dayjs": "^1.10.6",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-async-handler": "^1.1.4",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "~1.9.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pino": "^7.0.0-rc.1",
    "pino-pretty": "^5.1.2",
    "sequelize": "^6.6.5"
